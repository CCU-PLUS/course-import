{
  "name": "ccu-plus/course-import",
  "description": "Provide artisan commands for core module to import course data to database from National Chung Cheng University.",
  "keywords": ["ccu-plus"],
  "version": "1.0.0",
  "license": "AGPL-3.0-only",
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-zlib": "*",
    "ccu-plus/eloquent-orm": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/console": "^6.2",
    "illuminate/database": "^6.2",
    "illuminate/support": "^6.2",
    "overtrue/pinyin": "^4.0",
    "paquettg/php-html-parser": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ccu-plus/eloquent-orm"
    }
  ],
  "autoload": {
    "psr-4": {
      "CCUPLUS\\CourseImport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CCUPLUS\\CourseImport\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CCUPLUS\\CourseImport\\ServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
